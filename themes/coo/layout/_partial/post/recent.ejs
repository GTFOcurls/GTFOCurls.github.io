<h2 class="my-6 text-xl text-slate-700 dark:text-slate-300">Recent <span class="text-slate-400">Cheatsheet</span></h2>
<section class="w-full grid grid-cols-2 gap-2 md:gap-4">
    <% site.posts.sort('date', -1).filter(post => post.layout !== "note").limit(theme.recent_posts_limits).each(function(post){ %>
        <%- partial('_partial/post/popular_card', {post: post}) %>
    <% }) %>
</section>
